{
	"name": "Overdrive",
	"colors": {
		// Editor
		"menu.background": "#141414",
		"editor.background": "#101010",
		"editor.foreground": "#FAFBFC",
		"editor.selectionBackground": "#FFFFFF25",
		"editor.selectionHighlightBackground": "#FFFFFF25",
		"editorLineNumber.foreground": "#505050",
		"editorGroupHeader.tabsBackground": "#101010",
		"editorWidget.background": "#101010",
		"editorWarning.foreground": "#FFC799",
		"editorError.foreground": "#FF8080",
		"editorOverviewRuler.border": "#101010",
		"editorGutter.addedBackground": "#99FFE4",
		"editorGutter.deletedBackground": "#FF8080",
		"editorGutter.modifiedBackground": "#FFC799",
		"diffEditor.insertedTextBackground": "#99FFE415",
		"diffEditor.insertedLineBackground": "#99FFE415",
		"diffEditor.removedTextBackground": "#FF808015",
		"diffEditor.removedLineBackground": "#FF808015",
		"editorInlayHint.foreground": "#A0A0A0",
		"editorInlayHint.background": "#1C1C1C",
		// Sidebar
		"sideBar.background": "#101010",
		"sideBarTitle.foreground": "#01A3F4",
		"sideBarSectionHeader.foreground": "#A0A0A0",
		"sideBarSectionHeader.background": "#101010",
		// Activity bar
		"activityBar.background": "#101010",
		"activityBar.foreground": "#A0A0A0",
		"activityBarBadge.background": "#00FF9C",
		"activityBarBadge.foreground": "#000",
		// Title bar
		"titleBar.activeBackground": "#101010",
		"titleBar.inactiveBackground": "#101010",
		"titleBar.activeForeground": "#FAFBFC",
		"titleBar.inactiveForeground": "#707070",
		// Tab
		"tab.border": "#101010",
		"tab.activeBackground": "#101010",
		// "tab.activeBorder": "#FFFF00",
		"tab.inactiveBackground": "#101010",
		// Status bar
		"statusBar.debuggingForeground": "#FFF",
		"statusBar.debuggingBackground": "#FF7300",
		"statusBar.background": "#101010",
		"statusBar.foreground": "#A0A0A0",
		"statusBarItem.remoteBackground": "#00FF9C",
		"statusBarItem.remoteForeground": "#000",
		// Change explorer active item color
		"list.activeSelectionForeground": "#FFFF00",
		"menu.selectionBackground": "#333366",
		"list.inactiveSelectionBackground": "#333366",
		// Command Palette background and text
		"quickInput.background": "#141414",
		"quickInput.foreground": "#FFFFFF",
		"quickInputList.focusBackground": "#333366",
		"quickInputList.focusForeground": "#FFFF00",
		"quickInputList.focusIconForeground": "#FFFF00",
		// Global
		"badge.background": "#FFFF00",
		"badge.foreground": "#000",
		"button.background": "#FFFF00",
		"button.hoverBackground": "#FFFF0095",
		"button.foreground": "#000",
		"focusBorder": "#140e1a",
		"icon.foreground": "#A0A0A0",
		"input.background": "#1C1C1C",
		"list.activeSelectionBackground": "#333366",
		"list.hoverBackground": "#333366",
		"list.errorForeground": "#FF8080",
		"list.highlightForeground": "#FFFF00",
		"selection.background": "#666",
		// Brackets
		"editorBracketHighlight.foreground1": "#A0A0A0",
		"editorBracketHighlight.foreground2": "#A0A0A0",
		"editorBracketHighlight.foreground3": "#A0A0A0",
		"editorBracketHighlight.foreground4": "#A0A0A0",
		"editorBracketHighlight.foreground5": "#A0A0A0",
		"editorBracketHighlight.foreground6": "#A0A0A0",
		"editorBracketHighlight.unexpectedBracket.foreground": "#FF8080",
		// Links
		"textLink.foreground": "#FFC799",
		"textLink.activeForeground": "#FFCFA8",
		// Tooltips
		"editorHoverWidget.background": "#161616",
		"editorHoverWidget.border": "#282828",
		// Scrollbar
		"scrollbarSlider.background": "#34343480",
		"scrollbarSlider.hoverBackground": "#343434",
		// Settings
		"settings.modifiedItemIndicator": "#FFC799"
  	},
	"tokenColors": [
		{
			"name": "Default UI and plain text",
			"settings": {
				"foreground": "#D5CED9"
			}
		},
			{
			"name": "Comments and diff metadata",
			"scope": [
				"comment",
				"markup.quote.markdown",
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			"name": "Strings and inserted text",
			"scope": [
				"string",
				"text.html.php string",
				"markup.inline.raw",
				"markup.inserted",
				"punctuation.definition.string",
				"punctuation.definition.markdown",
				"text.html meta.embedded source.js string",
				"text.html.php punctuation.definition.string",
				"text.html meta.embedded source.js punctuation.definition.string",
				"text.html punctuation.definition.string",
				"text.html string"
			],
			"settings": {
				"foreground": "#00FF9C"
			}
		},
		{
			"name": "Constants, numbers, and bold text",
			"scope": [
				"markup.bold",
				"constant.numeric",
				"meta.group.regexp",
				"constant.other",
				"support.constant",
				"entity.other.attribute-name",
				"support.type.exception.python",
				"source.css keyword.other.unit",
				"variable.other.object.property.js.jsx",
				"variable.other.object.js"
			],
			"settings": {
				"foreground": "#FEBA17"
			}
		},
		{
			"name": "Keywords, italic elements, and types",
			"scope": [
				"keyword",
				"storage",
				"markup.italic",
				"meta.link",
				"meta.image",
				"source.js support.type"
			],
			"settings": {
				"foreground": "#01A3F4",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Italic user-defined and Python function names",
			"scope": [
				"entity.name.function",
				"meta.function-call.generic.python"
			],
			"settings": {
				"foreground": "#FEBA17",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Blade echo and built-in function calls",
			"scope": [
				"support.function",
				"meta.function.echo.blade"
			],
			"settings": {
				"foreground": "#E50046"
			}
		},
		{
			"name": "Variables, tags, and property names",
			"scope": [
				"variable",
				"support.variable",
				"entity.name.tag.yaml",
				"constant.character.entity.html",
				"source.css entity.name.tag.reference",
				"source.css entity.other.attribute-name.parent-selector",
				"meta.structure.dictionary.json support.type.property-name",
				"beginning.punctuation.definition.list.markdown"
			],
			"settings": {
				"foreground": "#A1E3F9"
			}
		},
		{
			"name": "Attribute names italicized",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function and attribute names non-italic",
			"scope": [
				"markup.list",
				"text.xml string",
				"meta.at-rule.extend",
				"meta.at-rule.extend support.constant",
				"entity.other.inherited-class",
				"entity.other.keyframe-offset.css",
				"text.html.markdown string.quoted",
				"entity.other.attribute-name.class.jade",
				"source.css entity.other.attribute-name",
				"text.xml punctuation.definition.string"
			],
			"settings": {
				"foreground": "#FEBA17"
			}
		},
		{
			"name": "Embedded code block punctuation",
			"scope": [
				"punctuation.definition.interpolation",
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php",
				"punctuation.section.embedded.begin.ruby",
				"punctuation.section.embedded.end.ruby",
				"punctuation.definition.template-expression",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#E50046",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown punctuation",
			"scope": [
				"punctuation.definition.metadata.markdown",
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown"
			],
			"settings": {
				"foreground": "#D5CED9"
			}
		},
		{
			"name": "Operators, classes, and deletions",
			"scope": [
				"support.class",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#E83F25"
			}
		},
		{
			"name": "Regular expressions and changed diff",
			"scope": [
				"string.regexp",
				"markup.changed"
			],
			"settings": {
				"foreground": "#7cb7ff"
			}
		},
		{
			"name": "Special keywords like this/self and headings",
			"scope": [
				"markup.heading",
				"variable.language.this.js",
				"variable.language.special.self.python"
			],
			"settings": {
				"foreground": "#ff00aa"
			}
		}
    ]
}
